<div class="row">
  <div class="col-md-2">

  </div>
  <div class="col-md-8">
    <div class="well">
      <section class="feature_article">
        <div class="image_wrapper aspect-ratio-16-9">
          <img src="<%= @event.image_url %>"  width="500px" class="center-block">
        </div>

        <div class = "text-center">
        <h1><%= @event.name %></h1>
        <p><%= @event.location %></p>
        <p><%= @event.date %></p>
        <p><%= @event.category %></p>
        <p><%= @event.description %></p>
      </div>
      </section>
    </div>
  </div>
  <div class="col-md-2">

  </div>
</div>
