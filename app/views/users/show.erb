<h1> <%= @user.to_s %> </h1>
<img src= <%= @user.picture_url %> alt="Profile picture" height="200">
<h3> <%= @user.age %>, <%= @user.gender %> </h3>
<h3> City: <%= @user.city %> </h3>
<h3> Height: <%= @user.feet_height %> </h3>
<h3>Interested In:</h3>
<ul>
<% if @user.genderqueer == true %>
<li><%= "trans or genderqueer people"  %></li>
<% end %>

<% if @user.men == true %>
<li> <%= "men"  %></li>
<% end %>

  <% if @user.women == true %>
  <li><%= "women"  %> </li>
  <% end %>
</ul>

<%= link_to 'Edit your preferences', edit_user_path(@user) %>
