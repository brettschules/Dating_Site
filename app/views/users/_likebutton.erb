<% if current_user == @user %>
<% elsif Match.where(liked_id: current_user.id, likes_id: @user.id ) == []  %>
  <%= form_tag( matches_path, method: "POST") do %>
  <%=hidden_field_tag(:liked_id, current_user.id) %>
  <%=hidden_field_tag(:likes_id, @user.id) %>
  <%= submit_tag("Like", class: "btn btn-danger btn-md") %>
  <% end %>
<% end %>
